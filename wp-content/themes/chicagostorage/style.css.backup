body {
  min-width: 320px;
}

#header {
  background: #ed3138;
  padding: 0 0 15px;
}
#header .container {
  margin: 0 auto;
  max-width: 320px;
  padding: 0 0 0 14px;
}
#header .call {
  float: right;
  background: white;
  border-radius: 0 0 0 8px;
  padding: 9px 12px 11px 10px;
  margin-bottom: 20px;
  font-size: 18px;
  text-transform: uppercase;
  text-decoration: none;
  vertical-align: middle;
}
#header .call i {
  font-size: 20px;
  vertical-align: top;
  margin-right: 0px;
}
#header .slogan {
  margin: 0 0 7px;
  padding-right: 10px;
  text-align: center;
  display: block;
  clear: both;
  color: #fff;
  font-size: 18px;
  font-weight: normal;
}
#header .slogan i {
  vertical-align: middle;
  font-size: 20px;
}
#header .slogan span {
  vertical-align: middle;
  text-transform: uppercase;
}
#header .logo {
  display: block;
  color: #fff;
  font-size: 48px;
  line-height: 1;
  font-weight: 700;
  text-shadow: 3px 3px 5px rgba(0, 0, 0, 0.75);
  text-align: center;
  text-transform: uppercase;
}
#header .logo a {
  color: #fff;
  text-decoration: none;
}

::-webkit-input-placeholder {
  color: black;
}

::-moz-placeholder {
  opacity: 1;
  color: black;
}

:-ms-input-placeholder {
  color: black;
}

.placeholder {
  color: black;
}

#main {
  margin: 0 auto;
  padding: 8px 15px 7px;
}
#main #content {
  padding-bottom: 25px;
}

.breadcrumbs ul {
  color: #448ccb;
  text-transform: uppercase;
  font-size: 14px;
  margin-bottom: 22px;
}
.breadcrumbs li {
  display: inline;
}
.breadcrumbs li + li:before {
  content: "\00bb";
  display: inline-block;
  margin-right: 3px;
}
.breadcrumbs a {
  text-decoration: none;
}
.breadcrumbs a:hover {
  text-decoration: underline;
}

.featured {
  border-top: 1px solid #999999;
  border-bottom: 1px solid #999999;
  margin: 0 -15px;
  padding: 12px 24px 11px;
  position: relative;
  margin-bottom: 47px;
}
.featured h2 {
  color: #ed3138;
  font-size: 18px;
  text-transform: uppercase;
  font-weight: 400;
  text-align: center;
  margin-bottom: 13px;
}
.featured .carousel .mask {
  width: 100%;
  overflow: hidden;
}
.featured .carousel .slideset {
  width: 99999px;
  position: relative;
}
.featured .carousel .slideset li {
  float: left;
  width: 290px;
}
.featured .carousel img {
  display: block;
  border: 1px solid #666666;
  padding: 1px;
  margin-right: 27px;
  margin: 0 auto;
  -webkit-box-sizing: content-box;
  -moz-box-sizing: content-box;
  box-sizing: content-box;
}
.featured .carousel .holder {
  text-align: center;
  padding-top: 12px;
  font-size: 18px;
  line-height: 21px;
}
.featured .carousel .holder .title {
  color: #448ccb;
  display: block;
}
.featured .carousel .holder p {
  margin: 0 0 13px;
}
.featured .carousel .btn {
  position: relative;
  z-index: 1;
  border: 1px solid #448ccb;
  padding: 9px 15px;
  background: #fff;
  color: #fff;
  font-weight: bold;
  display: inline-block;
  text-decoration: none;
  text-transform: uppercase;
  font-size: 18px;
}
.featured .carousel .btn:before {
  position: absolute;
  content: '';
  z-index: -1;
  top: 1px;
  bottom: 1px;
  right: 1px;
  left: 1px;
  background: #448ccb;
}
.featured .carousel .btn i {
  vertical-align: baseline;
  font-size: 18px;
}
.featured .carousel .btn:hover {
  opacity: .9;
}
.featured .carousel .paging {
  text-align: center;
  position: absolute;
  left: 0;
  right: 0;
  bottom: -38px;
}
.featured .carousel .paging ul {
  display: inline-block;
}
.featured .carousel .paging li {
  float: left;
  margin: 0 6px 0 1px;
}
.featured .carousel .paging li.active a, .featured .carousel .paging li:hover a {
  background: #448ccb;
  color: #fff;
}
.featured .carousel .paging a {
  width: 20px;
  height: 20px;
  border-radius: 50%;
  border: 1px solid #448ccb;
  background: #fff;
  display: block;
  overflow: hidden;
  text-indent: -9999px;
}

.search-box {
  margin-bottom: 25px;
}
#content .search-box {
  padding-top: 6px;
  margin-bottom: 18px;
}
.search-box h2 {
  text-transform: uppercase;
  margin-bottom: 3px;
}
.search-box form {
  margin-bottom: 17px;
}
.search-box [type="search"] {
  border-radius: 8px;
  padding: 6px 11px;
  font-size: 17px;
  display: block;
  margin-bottom: 12px;
  width: 100%;
  border-color: #1c3654;
  font-family: Montserrat, Arial, sans-serif;
  color: #999999;
}
.search-box [type="search"]::-webkit-input-placeholder {
  color: #999999;
}
.search-box [type="search"]::-moz-placeholder {
  opacity: 1;
  color: #999999;
}
.search-box [type="search"]:-ms-input-placeholder {
  color: #999999;
}
.search-box button {
  display: inline-block;
  text-decoration: none;
  border: none;
  vertical-align: top;
  color: #fff;
  font-weight: 700;
  text-transform: uppercase;
  border-bottom: 6px;
  background: #143048;
  border-radius: 8px;
  font-size: 16px;
  padding-bottom: 6px;
  text-align: center;
  float: right;
  white-space: nowrap;
}
.search-box button span {
  display: inherit;
  min-width: 0;
  padding: 8px 24px 5px 35px;
  background: #448ccb;
  vertical-align: top;
  border-radius: 8px;
}
.search-box button:hover {
  opacity: .9;
}
.search-box button i {
  font-size: 17px;
  margin-left: 5px;
  vertical-align: top;
  display: inline-block;
}
.search-box .view-map {
  font-size: 13px;
  text-transform: uppercase;
  text-decoration: none;
  font-weight: normal;
  display: inline-block;
  word-spacing: 2px;
  margin: 12px 0 0;
}
.search-box .view-map .icon-map {
  font-size: 25px;
  float: left;
  margin: -4px 7px 0 5px;
}
.search-box .view-map .icon-caret-right {
  font-size: 11px;
  vertical-align: baseline;
}

