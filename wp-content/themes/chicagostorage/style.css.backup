.details {
  color: #666;
  padding-bottom: 22px;
}
.details .info {
  line-height: 1.2;
  font-size: 12px;
  margin: 0 -15px 15px -5px;
}
.details .info .hold {
  padding: 0 0 15px;
}
.details .info .hold h1 {
  color: #ed3138;
  font-size: 14px;
  line-height: 1.4;
  margin: 0 0 -1px;
}
.details .info .hold p {
  margin: 0 0 -2px;
}
.details .info .row {
  overflow: hidden;
}
.details .info .call {
  float: right;
  background: #448ccb;
  text-transform: uppercase;
  width: 130px;
  color: #fff;
  font-size: 10px;
  line-height: 1.3;
  padding: 4px 8px 7px;
  margin: 6px 0 0;
  text-align: right;
}
.details .info .call strong {
  font-size: 16px;
  color: inherit;
  display: block;
  line-height: 1.2;
}
.details .info .rating {
  margin-top: 9px;
}
.details .info ul {
  font-size: 12px;
  overflow: hidden;
}
.details .info ul li {
  padding: 0 0 4px 8px;
  position: relative;
}
.details .info ul li:before {
  position: absolute;
  content: '';
  border-radius: 50%;
  width: 4px;
  height: 4px;
  top: 6px;
  left: 0;
  background-color: #666;
}
.details table {
  width: 100%;
  margin: 0;
}
.details table td {
  padding: 3px 5px 8px 10px;
  vertical-align: top;
}
.details table hr {
  display: block;
  border: none;
  border-top: 1px solid #448ccb;
  margin: 0 -5px 3px -10px;
}
.details table ul {
  font: 10px/1.2 Arial, Helvetica, sans-serif;
}
.details table ul li + li {
  padding-top: 5px;
}
.details table .btn-holder {
  text-align: center;
}
.details table .btn-holder .btn {
  display: inline-block;
  text-decoration: none;
  border: none;
  vertical-align: top;
  color: #fff;
  font-weight: 700;
  text-transform: uppercase;
  border-bottom: 6px;
  background: #143048;
  border-radius: 8px;
  font-size: 16px;
  padding-bottom: 6px;
  text-align: center;
}
.details table .btn-holder .btn span {
  display: inherit;
  min-width: 153px;
  padding: 6px 10px;
  background: #448ccb;
  vertical-align: top;
  border-radius: 8px;
}
.details table .btn-holder .btn:hover {
  opacity: .9;
}
.details .size {
  color: #448ccb;
  font-size: 30px;
  line-height: 1;
  display: block;
  margin-bottom: 2px;
}
.details .price-holder .price {
  vertical-align: baseline;
  margin-bottom: 3px;
}
.details .price-holder .price strong {
  color: #448ccb;
  font-size: 30px;
  line-height: 1;
  margin-right: 4px;
}
.details .price-holder .price span {
  font-size: 14px;
  line-height: 1.2;
}
.details .price-holder p {
  color: #ed3138;
  font-size: 12px;
  line-height: 14px;
  text-transform: uppercase;
}

.tabset {
  background: #8fafcb;
  text-transform: uppercase;
  font-size: 14px;
}
.tabset li {
  float: left;
}
.tabset li.active a, .tabset li:hover a {
  background: #448ccb;
}
.tabset a {
  float: left;
  text-decoration: none;
  color: #fff;
  padding: 11px 10px;
}

table td {
  border: 1px solid #448ccb;
  vertical-align: middle;
}

table tr:nth-child(even) td {
  background: #ecf7fe;
}

.carousel2 {
  position: relative;
  margin-bottom: 5px;
}
.carousel2 .mask {
  width: 100%;
  overflow: hidden;
}
.carousel2 .slideset {
  width: 99999px;
  position: relative;
}
.carousel2 .slideset li {
  float: left;
  width: 618px;
}
.carousel2 .slideset img {
  display: block;
  width: 100%;
  border-top: 1px solid #999999;
  border-bottom: 1px solid #999999;
}
.carousel2 [class*="btn-"] {
  position: absolute;
  top: 50%;
  width: 50px;
  height: 50px;
  color: #fff;
  text-align: center;
  background: #233540;
  text-decoration: none;
  font-size: 46px;
  line-height: 50px;
  margin-top: -20px;
}
.carousel2 [class*="btn-"].btn-next {
  right: 0;
  padding-left: 4px;
}
.carousel2 [class*="btn-"].btn-prev {
  left: 0;
  padding-right: 4px;
}
.carousel2 [class*="btn-"]:hover {
  opacity: .7;
}

.result {
  padding-bottom: 13px;
}
.result .item {
  margin-bottom: 5px;
  border-top: 1px solid #999999;
  border-bottom: 1px solid #999999;
  color: #666;
  font-size: 12px;
  line-height: 1.2;
  display: table;
